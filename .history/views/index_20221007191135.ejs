<!-- Header  -->
<%-include('include/_header')%>
<!-- /Header  -->

<!-- Main Site  -->
<main id="site-main">
  <div class="container">
    <div class="box-nav d-flex-justify-between">
      <a href="/add-user" class="border-shadow">
        <span class="text-gradient">
          New User <i class="fa-solid fa-user"></i>
        </span>
      </a>
    </div>
  </div>

  <!-- Form Handling  -->
  <form action="/" method="POST">
    <table class="table">
      <thead class="thead-dark">
        <tr>
          <th>ID</th>
          <th>Name</th>
          <th>Email</th>
          <th>Gender</th>
          <th>Status</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <%-include('include/_show')%>
      </tbody>
    </table>
  </form>
  <!-- /Form Handling -->
</main>

<!-- /Main Site  -->

<!-- Footer  -->

<%=users %>
<!-- /Footer  -->
